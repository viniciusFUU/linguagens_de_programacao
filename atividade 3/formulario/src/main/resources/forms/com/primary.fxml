<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="forms.com.controllers.FormularioJavaFXController">
   <TextField fx:id="nomeField" promptText="Nome"/>
   <TextField fx:id="idadeField" promptText="Idade"/>
   <TextField fx:id="documentoField" promptText="Documentos"/>
   <Button fx:id="enviarButton" text="Enviar" onAction="#handleEnviar"/>
   <Button fx:id="mostrarTodosButton" text="Mostrar Todos" onAction="#handleMostrarTodos"/>
</VBox>
